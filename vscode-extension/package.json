{
  "name": "autodocgen-vscode",
  "displayName": "AutoDocGen",
  "description": "Generate AI-based documentation and docstrings",
  "version": "0.0.1",
  "publisher": "Chirag Agrawal",
  "engines": {
    "vscode": "^1.70.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "autodocgen.run",
        "title": "Run AutoDocGen"
      },
      {
        "command": "autodocgen.runGui",
        "title": "Run AutoDocGen GUI"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "vscode:prepublish": "npm run compile"
  },
  "devDependencies": {
    "@types/node": "^18.18.0",
    "typescript": "^5.3.3",
    "@types/vscode": "^1.70.0"
  
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chirag-agrawal24/autodocgen.git"
  }
}
